
<configuration>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%m %n</pattern>
        </encoder>
    </appender>
    <logger name="com.ing">
        <level value="INFO" />
    </logger>

    <root>
        <level value="ERROR" />
        <appender-ref ref="CONSOLE" />
    </root>
</configuration>


